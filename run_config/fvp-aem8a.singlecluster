generate_lava_job() {
	local model="base-aemv8a"

	model="$model" \
		cluster_0_num_cores="4" \
		cluster_1_num_cores="0" \
		cluster_2_num_cores="0" \
		cluster_3_num_cores="0" \
		gen_model_params

	model="$model" gen_fvp_yaml
}
